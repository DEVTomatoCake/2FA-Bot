{
	"parserOptions": {
		"ecmaVersion": 2021
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"prefer-arrow-callback": 2,
		"no-useless-concat": 2,
		"no-shadow-restricted-names": 2,
		"no-const-assign": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-invalid-regexp": 2,
		"valid-typeof": 2,
		"no-duplicate-case": 2,
		"no-regex-spaces": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": 2,
		"no-useless-rename": 2,
		"no-useless-escape": 2,
        "no-unused-expressions": [ 2, { "allowShortCircuit": true } ],
        "no-redeclare": 1,
        "quotes": [2, "double", { "allowTemplateLiterals": false, "avoidEscape": true }],
		"no-empty-pattern": 2,
		"no-duplicate-imports": 2,
		"no-fallthrough": 2,
		"no-func-assign": 2,
		"no-import-assign": 2,
		"no-loss-of-precision": 2,
		"no-new-symbol": 2,
		"no-obj-calls": 2,
		"no-self-assign": 2,
		"no-unreachable": 1,
		"no-unreachable-loop": 2,
		"no-unsafe-finally": 2,
		"no-unused-vars": [1, { "argsIgnorePattern": "updateSlashcommands" }],
		"use-isnan": 2,
		"no-invalid-this": 2,
		"no-useless-computed-key": 1,
		"no-useless-return": 2,
		"no-useless-catch": 2
	}
}
